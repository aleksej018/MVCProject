<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="~/Content/fontawesome.css">
    <link rel="stylesheet" href="~/Content/templatemo-digimedia-v1.css">
    <link rel="stylesheet" href="~/Content/animated.css">
    <link rel="stylesheet" href="~/Content/owl.css">
</head>
<body>
    <div>
        <div id="js-preloader" class="js-preloader">
            <div class="preloader-inner">
                <span class="dot"></span>
                <div class="dots">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </div>
        </div>
        <!-- ***** Preloader End ***** -->
        <!-- Pre-header Starts -->
        <div class="pre-header">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-sm-8 col-7">
                        <ul class="info">
                            <li><a href="#"><i class="fa fa-envelope"></i>komponente365@gmail.com</a></li>
                            <li><a href="#"><i class="fa fa-phone"></i>064-020-0340</a></li>
                        </ul>
                    </div>
                    <div class="col-lg-4 col-sm-4 col-5">
                        <ul class="social-media">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-behance"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- Pre-header End -->
        <!-- ***** Header Area Start ***** -->
        <header class="header-area header-sticky wow slideInDown" data-wow-duration="0.75s" data-wow-delay="0s">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <nav class="main-nav">
                            <!-- ***** Logo Start ***** -->
                            <a class="logo">
                                <h1 class="logo_naslov">Prodavnica</h1>
                            </a>
                            <!-- ***** Logo End ***** -->
                            <!-- ***** Menu Start ***** -->
                            <ul class="nav">
                                <li class="scroll-to-section">@Html.ActionLink("Pocetna", "Index", "Site")</li>
                                <li class="scroll-to-section">@Html.ActionLink("Unos procesora", "UnosProcesor" ,"Site")</li>
                                <li class="scroll-to-section">@Html.ActionLink("Unos graficke", "UnosGraficka", "Site")<li>
                                <li class="scroll-to-section"></li>
                            </ul>
                            <a class='menu-trigger'>
                                <span>Menu</span>
                            </a>
                            <!-- ***** Menu End ***** -->
                        </nav>
                    </div>
                </div>
            </div>
        </header>

        @RenderBody()
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/owl-carousel.js"></script>
    <script src="~/Scripts/animation.js"></script>
    <script src="~/Scripts/imagesloaded.js"></script>
    <script src="~/Scripts/custom.js"></script>
</body>
</html>
